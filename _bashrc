# linux
alias la='ls -la'
alias ll='ls -l'

# docker
alias dcrun='docker-compose run --rm'
alias dcdel='docker ps -aq -f "status=exited" | xargs docker rm -v && docker images -q -f "dangling=true" | xargs docker rmi && docker volume ls -qf dangling=true | xargs docker volume rm'

# vim
alias ctags="`brew --prefix`/bin/ctags"

# git
alias gitlog='git log --all --oneline --graph'
